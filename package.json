{
	"name": "gauzy-pulumi",
	"version": "0.1.0",
	"description": "Gauzy Pulumi",
	"license": "GPL-3.0",
	"homepage": "https://gauzy.co",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ever-co/gauzy-pulumi.git"
	},
	"bugs": {
		"url": "https://github.com/ever-co/gauzy-pulumi/issues"
	},
	"private": true,
	"author": {
		"name": "Ever Co. LTD",
		"email": "ever@ever.co",
		"url": "https://ever.co"
	},
	"scripts": {
		"tslint-check": "tslint-config-prettier-check ./tslint.json",
		"precommit": "yarn lint-staged",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"commit": "git-cz",
		"commit:lint": "commitlint -E HUSKY_GIT_PARAMS",
		"format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write && git status",
		"semantic-release": "semantic-release",
		"snyk-protect": "snyk protect",
		"prepare": "yarn run snyk-protect"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.1.0",
		"@commitlint/config-conventional": "^8.1.0",
		"@semantic-release/changelog": "^3.0.4",
		"@semantic-release/git": "^7.0.16",
		"@semantic-release/github": "^5.4.3",
		"@semantic-release/npm": "^5.1.15",
		"@types/node": "^12.7.4",
		"@types/pg": "^7.11.1",
		"@types/uuid": "^3.4.5",
		"commitizen": "^4.0.3",
		"conventional-changelog": "^3.1.4",
		"conventional-changelog-cli": "^2.0.23",
		"cz-conventional-changelog": "^3.0.2",
		"husky": "^3.0.9",
		"lint-staged": "^10.0.0-0",
		"prettier": "^1.18.2",
		"prettier-tslint": "^0.4.2",
		"pretty-quick": "^2.0.0",
		"rxjs-tslint": "^0.1.7",
		"semantic-release": "^15.13.24",
		"snyk": "^1.236.0",
		"eslint": "^6.5.1",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-language-service": "^0.9.9"
	},
	"dependencies": {
		"@pulumi/aws": "^1.1.0",
		"@pulumi/awsx": "^0.18.10",
		"@pulumi/cloud": "^0.18.1",
		"@pulumi/cloud-aws": "^0.18.1",
		"@pulumi/cloudflare": "^0.17.14",
		"@pulumi/datadog": "0.18.11",
		"@pulumi/docker": "^0.17.4",
		"@pulumi/eks": "^0.18.14",
		"@pulumi/pulumi": "^1.1.0",
		"dotenv": "^8.1.0",
		"pg": "^7.12.1",
		"uuid": "^3.3.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"packages/**/*.{ts,json}": [
			"npm run format",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "pretty-quick --no-verify --staged"
		}
	},
	"snyk": true
}
